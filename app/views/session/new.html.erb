<h1>Log In</h1>
<%= form_tag session_index_path do %>
  <div class="input-group">
    <%= label_tag :email %><br/>
    <%= text_field_tag :email %>
  </div>
  <div class="input-group">
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <div class="input-group">
    <%= submit_tag "Log In" %>
  </div>
<% end %>
